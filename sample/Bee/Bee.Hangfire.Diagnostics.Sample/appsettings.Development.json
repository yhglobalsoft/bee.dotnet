{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "Hangfire": "localhost:6379,defaultdatabase=8"
  },
  "ElasticApm": {
    "LogLevel":  "Debug",
    "ServerUrls": "http://localhost:8200",
    "TransactionSampleRate": 1.0,
    "CaptureBody": "all",
    "CaptureBodyContentTypes": "application/x-www-form-urlencoded*, text/*, application/json*, application/xml*",
    "CloudProvider": "none",
    "EnableOpenTelemetryBridge": true
  }
}
